<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.CatchJob.dao.MemberDao">
	<insert id="insertOne" parameterType="member">
		INSERT INTO MEMBER_TB
		VALUES
		(#{MBER_IDX},#{MBER_ID},#{MBER_PW},#{MBER_TYPE},#{USE_FL},#{REG_DT},#{LAST_DT})
	</insert>
	<update id="updateOne" parameterType="member">
		<!-- 업데이트 -->
	</update>
	<delete id="deleteOne" parameterType="String">
		<!-- 딜리트 -->
	</delete>
	<select id="selectOne" parameterType="String" resultType="member">
		SELECT MBER_IDX
		FROM MEMBER_TB
	</select>
	<select id="selectAll"  resultType="member">
		SELECT *
		FROM MEMBER_TB
	</select>
	<update id="updateLastDate" parameterType="member">
		UPDATE MEMBER_TB SET
		#{LAST_DT} = SYSDATE
	</update>
	<select id="selectById" parameterType="String" resultType="member">
		SELECT * FROM MEMBER_TB WHERE MBER_ID = #{MBER_ID}
	</select>
</mapper>